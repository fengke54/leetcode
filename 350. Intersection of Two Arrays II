class Solution:
    def intersect(self, nums1: List[int], nums2: List[int]) -> List[int]:
        result = []
        if len(nums1) > len(nums2):
            for i in nums1:
                if i in nums2:
                    result.append(i)
                    nums2.remove(i) # in this case you can repeat the process
        else: 
            for i in nums2:
                if i in nums1:
                    result.append(i) # append only adds one element, not for repeating
                    nums1.remove(i)
        return result 
